{
  "compileOnSave": true,
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "alwaysStrict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": [
    "src"
  ]
}
